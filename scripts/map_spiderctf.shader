//SPAWN PLACE

textures/map_spiderctf/spawn_place_01
{
	qer_editorimage textures/map_spiderctf/spawn_place_01.tga
	surfaceparm	nomarks   
    surfaceparm	trans
	surfaceparm	nonsolid
    surfaceparm pointlight
	nopicmip
	{
		map textures/map_spiderctf/spawn_place_01.tga
		rgbGen identity
		blendFunc add
		tcmod rotate 10
	}
	
}

