textures/proceduralx/wall-fruitconcrete_red
{
	qer_editorimage textures/proceduralx/wall/fruitconcrete_red
	dpoffsetmapping - 0.5
	q3map_bouncescale 1.25


	{
		map textures/proceduralx/wall/fruitconcrete_red
				
	}

	{
		map $lightmap
		rgbGen identity
		tcGen lightmap
		blendfunc filter
	}
}


