textures/decals/scorch01
{
	qer_editorimage textures/decals/scorch01

	noPicMip
	noMipMaps
	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm nodlight
	polygonOffset
	sort 6
	cull none

	{
		map textures/decals/scorch01
		blendFunc GL_DST_COLOR GL_SRC_COLOR
		rgbgen identity
	}

	{
		map $lightmap
		blendFunc GL_ZERO GL_ONE
	}
}

