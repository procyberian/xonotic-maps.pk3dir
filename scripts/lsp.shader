

textures/lsp/ctf-redlogo
{
	qer_editorimage textures/lsp/ctf/redlogo.tga
	
	q3map_bounceScale 1.25


	{
		map textures/lsp/ctf/redlogo.tga
		

	}
	{
		map $lightmap
		rgbGen identity
		tcGen lightmap
		blendfunc filter
	}
}

