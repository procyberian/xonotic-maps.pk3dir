models/soultrees/soulbrancha
{
	cull none
	surfaceparm trans
	surfaceparm alphashadow
	dpglossintensitymod 1
	dpglossexponentmod 1
	q3map_bouncescale 3
	{
		map "models/soultrees/soulbrancha"
		alphafunc GE128
	}
	{
		map $lightmap
		rgbGen identity
		tcGen lightmap
		blendfunc filter
	}
}

