models/soultrees/soultrunkb2
{
	dpmeshcollisions
	dpglossintensitymod 1
	dpglossexponentmod 1
	dpoffsetmapping 5
	{
		map "models/soultrees/soultrunkb2"
	}
	{
		map $lightmap
	}
}

