//Jumppad1 blue
textures/effects_jumppad/jumppadfx1_a
{
	qer_editorimage textures/effects_jumppad/jumppadfx1_a.tga
	surfaceparm nolightmap
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	cull none
	qer_trans 0.6
	{
		map textures/effects_jumppad/jumppadfx1_a.tga
		blendfunc add
		tcMod scroll 0 2
	}
}
