//Jumppad1 blue
textures/fx/jumppadfx1-a
{
	qer_editorimage textures/fx/jumppad/jumppadfx1_a.tga
	surfaceparm nolightmap
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	cull none
	qer_trans 0.6
	{
		map textures/fx/jumppad/jumppadfx1_a.tga
		blendfunc add
		tcMod scroll 0 2
	}
}
