textures/logos/logo01_blue
{
	qer_editorimage textures/logos/logo01/logo01_blue.tga

	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm nodlight
	surfaceparm nolightmap

	polygonOffset
	sort 6
	cull none

	{
		map textures/logos/logo01/logo01_blue.tga
		blendFunc filter
		alphaFunc GT0
		rgbgen identity
	}
}

