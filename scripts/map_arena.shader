textures/map_arena/floor00invg_g
{
	qer_editorimage textures/map_arena/gfx/floor00invg.tga

	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm nodlight
	surfaceparm nolightmap

	polygonOffset
	sort 6
	cull none

	{
		map textures/map_arena/gfx/floor00invg.tga
		blendFunc blend
		alphaFunc GT0
		rgbgen identity
	}
}

