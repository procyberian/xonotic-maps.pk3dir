{
	qer_editorimage textures/map_arena/warpzone/blueqer.tga
//	surfaceparm nonsolid	//NO! SOLID! Else it's epic fail
	surfaceparm nomarks
	q3map_surfacelight 2500
	cull none
	{
		map textures/effects_warpzone/blueedge.tga
		blendfunc GL_SRC_ALPHA GL_ONE
